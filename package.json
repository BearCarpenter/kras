{
  "name": "kras",
  "version": "0.8.0",
  "description": "Efficient server proxying and mocking in Node.js.",
  "main": "dist/server/kras.js",
  "types": "dist/server/kras.d.ts",
  "bin": {
    "kras": "dist/server/kras-cli.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FlorianRappl/kras.git"
  },
  "bugs": {
    "url": "https://github.com/FlorianRappl/kras/issues"
  },
  "scripts": {
    "test:unit": "cross-env NODE_ENV=test jest src",
    "test:watch": "npm run test:unit -- --watch",
    "test": "npm run test:unit",
    "start": "ts-node src/server/kras-cli.ts",
    "watch": "nodemon --exec ts-node -- src/server/kras-cli.ts",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prettier": "prettier --config prettier.config.js --write src/**/*.{ts,tsx}",
    "build:client": "cross-env NODE_ENV=production webpack -p",
    "build:server": "tsc --project tsconfig.server.json",
    "build:utility": "tsc --project tsconfig.utility.json",
    "build": "npm run build:server && npm run build:utility && npm run build:client"
  },
  "author": "Florian Rappl <mail@florian-rappl.de>",
  "contributors": [],
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^2.4.2",
    "chokidar": "^2.1.2",
    "express": "^4.16.4",
    "express-ws": "^4.0.0",
    "request": "^2.88.0",
    "send": "^0.16.2",
    "uuid": "^3.3.2",
    "ws": "^6.1.4",
    "yargs": "^13.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chokidar": "^1.7.5",
    "@types/enzyme": "^3.1.18",
    "@types/enzyme-adapter-react-16": "^1.0.4",
    "@types/express": "^4.16.1",
    "@types/express-ws": "^3.0.0",
    "@types/jest": "^24.0.6",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "@types/react-router": "^4.4.4",
    "@types/react-router-dom": "^4.3.1",
    "@types/reactstrap": "^7.1.3",
    "@types/request": "^2.48.1",
    "@types/send": "^0.14.4",
    "@types/uuid": "^3.4.4",
    "@types/ws": "^6.0.1",
    "@types/yargs": "^12.0.9",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.1.0",
    "bootstrap": "~4.3.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "jest-enzyme": "^7.0.1",
    "mixin-deep": "^2.0.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "react": "^16.8.3",
    "react-ace": "^6.4.0",
    "react-dom": "^16.8.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^2.9.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0"
  }
}
