{
  "name": "kras",
  "version": "0.7.1",
  "description": "Efficient server proxying and mocking in Node.js.",
  "main": "dist/server/kras.js",
  "types": "dist/server/kras.d.ts",
  "bin": {
    "kras": "dist/server/kras-cli.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FlorianRappl/kras.git"
  },
  "bugs": {
    "url": "https://github.com/FlorianRappl/kras/issues"
  },
  "scripts": {
    "test:unit": "cross-env NODE_ENV=test jest src",
    "test:watch": "npm run test:unit -- --watch",
    "test": "npm run test:unit",
    "start": "ts-node src/server/kras-cli.ts",
    "watch": "nodemon --exec ts-node -- src/server/kras-cli.ts",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prettier": "prettier --config prettier.config.js --write src/**/*.{ts,tsx}",
    "build:client": "cross-env NODE_ENV=production webpack -p",
    "build:server": "tsc --project tsconfig.server.json",
    "build:utility": "tsc --project tsconfig.utility.json",
    "build": "npm run build:server && npm run build:utility && npm run build:client"
  },
  "author": "Florian Rappl <rappl.florian@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "express": "^4.16.4",
    "express-ws": "^4.0.0",
    "request": "^2.88.0",
    "send": "^0.16.2",
    "uuid": "^3.3.2",
    "ws": "^6.1.2",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chokidar": "^1.7.5",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/express": "^4.16.0",
    "@types/express-ws": "^3.0.0",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.11",
    "@types/react-router": "^4.4.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/reactstrap": "^6.4.2",
    "@types/request": "^2.48.1",
    "@types/send": "^0.14.4",
    "@types/uuid": "^3.4.4",
    "@types/ws": "^6.0.1",
    "@types/yargs": "^12.0.1",
    "awesome-typescript-loader": "^3.5.0",
    "babel-jest": "^23.6.0",
    "bootstrap": "~4.1.2",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.4",
    "jest-enzyme": "^4.2.0",
    "mixin-deep": "^1.3.1",
    "nodemon": "^1.18.7",
    "prettier": "^1.12.1",
    "react": "^16.6.3",
    "react-ace": "^6.2.0",
    "react-dom": "^16.6.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^6.5.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.20.3",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.2",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^3.1.10"
  }
}
