{
  "name": "kras",
  "version": "0.10.2",
  "description": "Efficient server proxying and mocking in Node.js.",
  "main": "dist/server/index.js",
  "types": "dist/server/index.d.ts",
  "bin": {
    "kras": "dist/cli.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FlorianRappl/kras.git"
  },
  "bugs": {
    "url": "https://github.com/FlorianRappl/kras/issues"
  },
  "scripts": {
    "test:unit": "cross-env NODE_ENV=test jest src",
    "test:watch": "npm run test:unit -- --watch",
    "test": "npm run test:unit",
    "start": "ts-node src/cli.ts",
    "watch": "nodemon --exec ts-node -- src/cli.ts",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prettier": "prettier --config prettier.config.js --write src/**/*.{ts,tsx}",
    "build:server:decl": "dets --imports node express --files src/server/index.ts --types src/server/index.ts --out dist/server/index.d.ts",
    "build:server:code": "ncc build src/server/index.ts --out dist/server",
    "build:server": "npm run build:server:code && npm run build:server:decl",
    "build:utility": "ncc build utils/index.ts --out utils",
    "build:client": "cross-env NODE_ENV=production webpack -p",
    "build:cli": "tsc --project tsconfig.cli.json",
    "build": "npm run build:cli && npm run build:utility && npm run build:client && npm run build:server"
  },
  "author": "Florian Rappl <mail@florian-rappl.de>",
  "contributors": [],
  "license": "MIT",
  "dependencies": {
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chokidar": "^2.1.3",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.3",
    "@types/express-ws": "^3.0.0",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/reactstrap": "^8.4.2",
    "@types/request": "^2.48.4",
    "@types/send": "^0.14.5",
    "@types/uuid": "^7.0.2",
    "@types/ws": "^7.2.3",
    "@types/yargs": "^15.0.4",
    "@zeit/ncc": "^0.21.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^25.1.0",
    "body-parser": "^1.19.0",
    "bootstrap": "~4.4.1",
    "chalk": "^3.0.0",
    "chokidar": "^3.3.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "dets": "^0.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "jest-enzyme": "^7.1.2",
    "mixin-deep": "^2.0.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-ace": "^8.1.0",
    "react-dom": "^16.13.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "reactstrap": "^8.4.1",
    "request": "^2.88.2",
    "send": "^0.17.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.7.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.2.0",
    "typescript": "^3.8.3",
    "uuid": "^7.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "ws": "^7.2.1"
  }
}
